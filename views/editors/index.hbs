<div class="content">
    <h1>Liste des éditeurs</h1>

    <ul class="list">
        {{#each editors}}
            <li class="item" id="{{id}}">
                <a href="/editors/{{id}}" class="link">{{name}}</a> 
            </li>
        {{/each}}
    </ul>
    
    <a href="editors/new" class="link">Ajouter un éditeur</a>

</div>
